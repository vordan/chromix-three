{
	"manifest_version": 3,
	"name": "Chromix Three",
	"version": "1.2.0",
	"description": "Developer tool for Chrome control from command line",
	"author": "Vanco Ordanoski <vordan@infoproject.biz> (Infoproject LLC)",
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"48": "icons/icon-48.png"
	},
	"permissions": [
		"tabs",
		"alarms"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"action": {
		"default_popup": "popup.html",
		"default_title": "Chromix Three",
		"default_icon": {
			"16": "icons/icon-16.png",
			"32": "icons/icon-32.png",
			"48": "icons/icon-48.png"
		}
	},
	"background": {
		"service_worker": "service-worker.js"
	}
}
